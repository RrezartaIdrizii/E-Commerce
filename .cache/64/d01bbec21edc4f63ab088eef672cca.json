#393E46;\r\n  color: #fff;\r\n  padding: 20px;\r\n  text-align: center;\r\n}\r\n\r\nnav ul li form {\r\n  display: flex;\r\n}\r\n\r\nnav ul li form input[type=\"text\"] {\r\n  width: 150px;\r\n}\r\n\r\nnav ul li form button {\r\n  background-color: #00ADB5;\r\n  color: #fff;\r\n  border: none;\r\n  padding: 8px;\r\n  cursor: pointer;\r\n}\r\n\r\nnav {\r\n  background-color: #393E46;\r\n  color: #fff;\r\n  padding: 20px;\r\n  display: flex;\r\n  justify-content: space-between;\r\n}\r\n\r\n.navbar-logo img {\r\n  height: 70px;\r\n  vertical-align: middle;\r\n}\r\n\r\n.navbar-links ul {\r\n  list-style-type: none;\r\n  margin: 0;\r\n  padding: 0;\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\n.navbar-links ul li {\r\n  margin-right: 30px;\r\n}\r\n\r\n.navbar-links ul li a {\r\n  color: #fff;\r\n  text-decoration: none;\r\n}\r\n\r\n.home-entrance {\r\n  opacity: 0;\r\n  transform: translateY(-50px);\r\n  animation: homeEntrance 1s ease-out forwards;\r\n}\r\n\r\n@keyframes homeEntrance {\r\n  from {\r\n    opacity: 0;\r\n    transform: translateY(-50px);\r\n  }\r\n  to {\r\n    opacity: 1;\r\n    transform: translateY(0);\r\n  }\r\n}\r\n\r\n@keyframes motion {\r\n  0% { transform: translateY(0); }\r\n  50% { transform: translateY(-10px); }\r\n  100% { transform: translateY(0); }\r\n}\r\n\r\n#entrance {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  justify-content: center;\r\n  height: 100vh;\r\n  position: relative;\r\n  color: #fff;\r\n}\r\n\r\n#background-image {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  background-image: url('./public/img/wallpaper.jpeg');\r\n  background-size: cover;\r\n  background-position: center;\r\n  filter: blur(3px);\r\n  z-index: -1;\r\n}\r\n.home-entrance h1{\r\n  text-align: center;\r\n  color: #00ADB5;\r\n  \r\n}\r\n#entrance p {\r\n  text-align: center;\r\n  color: #686C72;\r\n}\r\n#entrance h1{\r\n    text-align: center;\r\n    color: #EEEEEE;\r\n    font-size: 60px;\r\n    animation: motion 2s ease-in-out infinite;\r\n}\r\n\r\naudio {\r\n  display: none;\r\n}\r\n\r\n.button-container {\r\n  display: flex;\r\n  justify-content: center;\r\n  margin-top: 20px;\r\n}\r\n\r\n.button-container button {\r\n  margin-right: 10px;\r\n  background-color: #00ADB5;\r\n  color: #fff;\r\n  border: none;\r\n  padding: 10px 20px;\r\n  font-size: 16px;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s ease;\r\n}\r\n\r\n.button-container button:hover {\r\n  background-color: #007C82;\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"a3dd66df2655f270e9fb8caf2fcfc108","cacheData":{"env":{}}}