endChild(row);\n\n    // Calculate the total price\n    total += item.price * item.quantity;\n  });\n\n  // Update the total price\n  cartTotal.innerText = total.toFixed(2);\n}\n\n// Function to clear the cart\nfunction clearCart() {\n  localStorage.removeItem('cartItems');\n  updateCart();\n}\n\n// Function to handle checkout\nfunction checkout() {\n  window.location.href = 'slider.html';\n}\n\n// Call the updateCart function when the page loads\nwindow.addEventListener('load', updateCart);</script>\r\n    <script src=\"/9a9569e9d73f33740eada95275da7f30.js\"></script>\r\n    \r\n</body>\r\n</html>\r\n"},"sourceMaps":null,"error":null,"hash":"21fddc7c2b5a4194970fd8a3f8668d88","cacheData":{"env":{}}}